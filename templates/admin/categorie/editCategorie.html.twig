{% extends "base.html.twig" %}

{% block title %} Modifier la Categorie{% endblock %}

{% block body %}
<div class="container">
    <h1 class="my-3">Modifier la categorie</h1>
    {{ form_start(form) }}
    {{ form_row(form.libelle, {'attr': {'class': 'mt-2'}}) }}
    {{ form_row(form.imageFile, {'attr': {'class': 'mt-2'}}) }}
    {{ form_row(form.active, {'attr': {'class': 'mt-2'}}) }}
    <button type="submit" class="btn btn-primary mt-3">Modifier</button>
    {{ form_end(form) }}
</div>
{% endblock %}